<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<VBox fx:controller="All.AddFoodController" xmlns:fx="http://javafx.com/fxml" prefHeight="200.0" prefWidth="350.0">
    <children>
        <GridPane alignment="center" hgap="10" vgap="10" VBox.vgrow="ALWAYS">
            <TextField  id="txt_search" fx:id="txt_search" GridPane.columnIndex="0" GridPane.rowIndex="2" prefHeight="20.0" prefWidth="150.0" promptText="To Search" />
            <Button  GridPane.columnIndex="1" GridPane.rowIndex="2" prefHeight="20.0" prefWidth="150.0" mnemonicParsing="false" text="Search" onAction="#goSearch"/>
            <ComboBox id="Foods" fx:id="Foods" editable="true"  GridPane.columnIndex="0" GridPane.rowIndex="3" prefHeight="20.0" prefWidth="150.0" promptText="Select Food" >
                <items>
                    <FXCollections fx:factory="observableArrayList">
                    </FXCollections>
                </items>
            </ComboBox>
            <TextField  id="quantity" fx:id="quantity" GridPane.columnIndex="1" GridPane.rowIndex="3" prefHeight="20.0" prefWidth="150.0" promptText="Quantity" />
            <ComboBox id="MealType" fx:id="MealType" editable="true"  GridPane.columnIndex="0" GridPane.rowIndex="4" prefHeight="20.0" prefWidth="150.0" promptText="Select Meal Type" >
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="Breakfast" />
                        <String fx:value="Lunch" />
                        <String fx:value="Dinner" />
                        <String fx:value="Snack" />
                    </FXCollections>
                </items>
            </ComboBox>
            <Button  GridPane.columnIndex="1" GridPane.rowIndex="5" prefHeight="20.0" prefWidth="150.0" mnemonicParsing="false" text="Add Food" onAction="#AddFoodsAction"/>
            <Button  GridPane.columnIndex="0" GridPane.rowIndex="5" prefHeight="20.0" prefWidth="150.0" mnemonicParsing="false" text="Go Back" onAction="#GoToDashButtonAction"/>
            <Label  id="errorMsg" fx:id="errorMsg" GridPane.columnIndex="0" GridPane.rowIndex="6" prefHeight="20.0" prefWidth="150.0" text=""/>
        </GridPane>
    </children>
</VBox>
