<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:controller="All.AddGoalController" xmlns:fx="http://javafx.com/fxml" prefHeight="400.0" prefWidth="350.0">
    <children>
        <GridPane alignment="center" hgap="10" vgap="10" VBox.vgrow="ALWAYS">
            <Label GridPane.columnIndex="0" GridPane.rowIndex="0" prefHeight="20.0" prefWidth="150.0" text="Setting a Goal" />
            <TextField  id="TargetWeight" fx:id="TargetWeight" GridPane.columnIndex="1" GridPane.rowIndex="1" prefHeight="20.0" prefWidth="150.0" promptText="Target Weight" />
            <DatePicker id="DateDue" fx:id="DateDue" GridPane.columnIndex="1" GridPane.rowIndex="2" prefHeight="20.0" prefWidth="150.0" promptText="Target Date" />
            <Button  GridPane.columnIndex="1" GridPane.rowIndex="3" prefHeight="20.0" prefWidth="150.0" mnemonicParsing="false" text="Set Goal" onAction="#AddWeightGoalButtonAction"/>
            <Button  GridPane.columnIndex="0" GridPane.rowIndex="3" prefHeight="20.0" prefWidth="150.0" mnemonicParsing="false" text="Go Back" onAction="#GoToDashButtonAction"/>
        </GridPane>
    </children>
</VBox>
